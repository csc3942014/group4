<div class="center-green-border">
    <center><h4>Thank you for your time, how'd you feel about it?</h4></center>
    <br>
    
    <%= form_for @test_session, :html => { :class => 'form-horizontal' } do |f| %>

        <div class="form-group">
            <%= f.label :ease_ranking, :class => 'control-label' %>
            <div class="controls">
                <center><%= f.text_field :ease_ranking, :class => 'form-control', :style => "width:50%;", :placeholder => "on a scale of 0-10" %></center>
            </div>
        </div>
        <div class="form-group">
            <%= f.label :fun_ranking, :class => 'control-label' %>
            <div class="controls">
                <center><%= f.text_field :fun_ranking, :class => 'form-control', :style => "width:50%;", :placeholder => "on a scale of 0-10" %></center>
            </div>
        </div>
        <div class="form-group">
            <%= f.label :accuracy_ranking, :class => 'control-label' %>
            <div class="controls">
                <center><%= f.text_field :accuracy_ranking, :class => 'form-control', :style => "width:50%;", :placeholder => "on a scale of 0-10" %></center>
            </div>
        </div>

        <%= f.submit 'Submit', :class => 'btn btn-primary' %>

    <% end %>

</div>
