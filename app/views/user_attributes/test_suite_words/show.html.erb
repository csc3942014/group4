<%- model_class = TestSuiteWord -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:test_suite_id) %>:</strong></dt>
  <dd><%= @test_suite_word.test_suite_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:word_id) %>:</strong></dt>
  <dd><%= @test_suite_word.word_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              test_suite_words_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_test_suite_word_path(@test_suite_word), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              test_suite_word_path(@test_suite_word),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
