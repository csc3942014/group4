<%- model_class = TestSession -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @test_session.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ease_ranking) %>:</strong></dt>
  <dd><%= @test_session.ease_ranking %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fun_ranking) %>:</strong></dt>
  <dd><%= @test_session.fun_ranking %></dd>
  <dt><strong><%= model_class.human_attribute_name(:accuracy_ranking) %>:</strong></dt>
  <dd><%= @test_session.accuracy_ranking %></dd>
  <dt><strong><%= model_class.human_attribute_name(:test_suite_id) %>:</strong></dt>
  <dd><%= @test_session.test_suite_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:test_unit_id) %>:</strong></dt>
    <dd><%= @test_session.keyboard_id %></dd>
    
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              test_sessions_path, :class => 'btn btn-default'  %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              test_session_path(@test_session),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
