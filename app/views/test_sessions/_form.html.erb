<%= form_for @test_session, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :user_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :user_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :test_number, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :test_number, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :start_time, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :start_time, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :end_time, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :end_time, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :ease_ranking, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ease_ranking, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :fun_ranking, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fun_ranking, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :accuracy_ranking, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :accuracy_ranking, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :test_suite_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :test_suite_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :test_unit_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :test_unit_id, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                test_sessions_path, :class => 'btn' %>
  </div>
<% end %>
