<!DOCTYPE html>
<html>
<head>
  <title>Group4</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
    
    <% if notice %>
      <p class="alert alert-success"><%= notice %></p>
    <% end %>
    <% if alert %>
      <p class="alert alert-danger"><%= alert %></p>
    <% end %>
    
</head>
<body>

<nav class="navbar navbar-fixed-top navbar-default" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapsable">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a href="/" class="navbar-brand">AR Analytics</a>
    </div>
    <div class="collapse navbar-collapse" id="navbar-collapsable">
      <ul class="nav navbar-nav foo" id="menu">

        <li><%= link_to "Home", home_page_path %></li>

	<li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">Analytics <b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li class="dropdown-header">Testing</li>
              <li><%= link_to "General", analytics_general_path %></li>
              <li><%= link_to "Specific", analytics_general_path %></li>
    	      <li><%= link_to "Comparatif", analytics_comparatif_path %></li>
          </ul>
        </li>

        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">User testing <b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li class="dropdown-header">Testing</li>
              <li><%= link_to "Testing", testing_path %></li>
              <li><%= link_to "Test Setup", test_setup_path %></li>
          </ul>
        </li>

        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">Raw Models <b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li class="dropdown-header">Testing</li>
              <li><%= link_to "Words", words_path %></li>
              <li><%= link_to "Test Units", test_units_path %></li>
              <li><%= link_to "Test Suites", test_suites_path %></li>
              <li><%= link_to "Test Suite Words", test_suite_words_path %></li>
              <li><%= link_to "Coordinates", coordinates_path %></li>
              <li><%= link_to "Keyboards", keyboards_path %></li>
            <li class="divider"></li>
              <li><%= link_to "User Infos", user_infos_path %></li>
              <li><%= link_to "User Roles", user_roles_path %></li>
              <li><%= link_to "Attributes", attributes_path %></li>
              <li><%= link_to "User Attributes", user_attributes_path %></li>
              <li><%= link_to "Roles", roles_path %></li>
          </ul>
        </li>

      <ul class="nav navbar-nav navbar-right" align="right">
        <li><a href="/logout">Log Out</a></li>
      </ul>
    </div>
  </div>
</nav>

<div class="container">
  <div class="span9"><%= yield %></div>
</div>

